package add_trigger;
use strict;
use warnings;
#use parent 'Blosxom::Plugin';
use Class::Method::Modifiers;

#__PACKAGE__->load_plugin( 'AddTrigger' );

sub start {
    #my ( $class, $str ) = @_;
    #$str;
    my $class = shift;
    my $str = shift;
    $str;
}

around start => sub {
    my $orig = shift;
    ucfirst $orig->( @_ );
};

1;
