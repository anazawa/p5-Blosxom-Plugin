package not_found;
use strict;
use warnings;
use base qw/Blosxom::Plugin/;

__PACKAGE__->load_plugin( 'DataSection' );

sub start { !$blosxom::static_entries }

sub last {
    my $class = shift;
    $blosxom::output = $class->get_data_section( '404.html' );
}

1;

__DATA__

@@ 404.html
404
Not
Found
